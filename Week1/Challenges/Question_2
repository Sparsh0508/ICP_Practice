class Solution {
    public int findContentChildren(int[] g, int[] s) {

        // worst Approach
        Arrays.sort(g);
        Arrays.sort(s);
        int i = 0;
        int j = 0;
        int cnt= 0;
        while(i<s.length && j<g.length){

            if(s[i] >= g[j]){
            j++;
                cnt++;
               int max= Math.max(cnt,i);;
            }
            i++;
        }
        return cnt;
    }
}
